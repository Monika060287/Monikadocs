Concurrent(
    Set(
        varRequestor,
        LookUp(
            'Environment Variable Values',
            'Environment Variable Definition'.'Display Name' = "envVar-IWH-RequestorList"
        ).Value
    ),
    Set(
        varLogistic,
        LookUp(
            'Environment Variable Values',
            'Environment Variable Definition'.'Display Name' = "envVar-IWH-LogisticList"
        ).Value
    ),
   
    Set(
        varGISITSD,
        LookUp(
            'Environment Variable Values',
            'Environment Variable Definition'.'Display Name' = "envVar-IWH-GISITSDList"
        ).Value
    ),
    
    Set(
        varBuyer,
        LookUp(
            'Environment Variable Values',
            'Environment Variable Definition'.'Display Name' = "envVar-IWH-BuyerList"
        ).Value
    ),
    Set(
        varFinance,
        LookUp(
            'Environment Variable Values',
            'Environment Variable Definition'.'Display Name' = "evVar-IWH-FinanceList"
        ).Value
    ),
    Set(
        varFacility,
        LookUp(
            'Environment Variable Values',
            'Environment Variable Definition'.'Display Name' = "envVar-IWH-FacilityManagerList"
        ).Value
    ),
    Set(
        varBTPSecurity,
        LookUp(
            'Environment Variable Values',
            'Environment Variable Definition'.'Display Name' = "envVar-IWH-BTPSecurityList"
        ).Value
    ),
     Set(
        varReceivingTeam,
        LookUp(
            'Environment Variable Values',
            'Environment Variable Definition'.'Display Name' = "envVar-IWH-ReceivingTeam"
        ).Value
    ),
    Set(
        varAzureStorageAccount,
        LookUp(
            'Environment Variable Values',
            'Environment Variable Definition'.'Display Name' = "envVar-IWH-AzureBlob_StorageAccountName"
        ).Value
    ),
    Set(
        varAzureSASTocken,
        LookUp(
            'Environment Variable Values',
            'Environment Variable Definition'.'Display Name' = "envVar-IWH-AzureBlob_SAS_Tocken"
        ).Value
    )
);
Set(
    varLoggedInUserGroup,
    If(
        CountRows(
            Filter(
                Office365Groups.ListGroupMembers(varBTPSecurity).value,
                mail = User().Email
            )
        ) > 0,
        "BTPSecurity",
        CountRows(
            Filter(
                Office365Groups.ListGroupMembers(varLogistic).value,
                mail = User().Email
            )
        ) > 0,
        "Logistic",
        CountRows(
            Filter(
                Office365Groups.ListGroupMembers(varFacility).value,
                mail = User().Email
            )
        ) > 0,
        "Facility",
        CountRows(
            Filter(
                Office365Groups.ListGroupMembers(varRequestor).value,
                mail = User().Email
            )
        ) > 0,
        "Requestor",
        CountRows(
            Filter(
                Office365Groups.ListGroupMembers(varBuyer).value,
                mail = User().Email
            )
        ) > 0,
        "Buyer",
        CountRows(
            Filter(
                Office365Groups.ListGroupMembers(varGISITSD).value,
                mail = User().Email
            )
        ) > 0,
        "GISITSD",
        CountRows(
            Filter(
                Office365Groups.ListGroupMembers(varFinance).value,
                mail = User().Email
            )
        ) > 0,
        "Finance",
         CountRows(
            Filter(
                Office365Groups.ListGroupMembers(varReceivingTeam).value,
                mail = User().Email
            )
        ) > 0,
        "ReceivingTeam",
        "Requestor"
    )
);

//Uncommented start for pagination
Set(
    varSTGPCount,
    If(
        varLoggedInUserGroup = "Logistic",
        CountRows(
            Filter(
                col_STGP_Part_Details,
                 IWH_STGP_RequestNo.STGP_Status = IWH_STGP_Status.'InProgress-With Logistics team'
                ||IWH_STGP_RequestNo.STGP_Status = IWH_STGP_Status.'Return Pending'||IWH_STGP_RequestNo.STGP_Status =IWH_STGP_Status.'GP Partial Return'
            )
        ),
        varLoggedInUserGroup = "GISITSD",
        CountRows(
            Filter(
                col_STGP_Part_Details,
                IWH_STGP_RequestNo.STGP_Status = IWH_STGP_Status.'InProgress-With GIS IT SD Team'
            )
        ),
        varLoggedInUserGroup = "Requestor",
        CountRows(
            Filter(
                col_STGP_Part_Details,
                IWH_STGP_RequestNo.STGP_Status = IWH_STGP_Status.'InProgress-With Requestor' || IWH_STGP_RequestNo.'Created By'.'Full Name' = User().FullName
            )
        ),
        varLoggedInUserGroup = "Finance",
        CountRows(
            Filter(
                col_STGP_Part_Details,
                IWH_STGP_RequestNo.STGP_Status = IWH_STGP_Status.'InProgress-With Finance Team' || IWH_STGP_RequestNo.STGP_Status = IWH_STGP_Status.'InProgress-With Finance & IMS Team'
            )
        ),
        //Commented for Receiving Team -Monika
        /*varLoggedInUserGroup = "BTPSecurity",
        CountRows(
            Filter(
                col_STGP_Part_Details,
                IWH_STGP_RequestNo.STGP_Status = IWH_STGP_Status.'InProgress-With BTP Security Team'
            )
        ),*/
        varLoggedInUserGroup = "Facility",
        CountRows(
            Filter(
                col_STGP_Part_Details,
                IWH_STGP_RequestNo.STGP_Status = IWH_STGP_Status.'InProgress-With Facility Manager'
            )
        ),
        varLoggedInUserGroup = "Buyer",
        CountRows(
            Filter(
                col_STGP_Part_Details,
                IWH_STGP_RequestNo.STGP_Status = IWH_STGP_Status.'InProgress-With IMS Team' || IWH_STGP_RequestNo.STGP_Status = IWH_STGP_Status.'InProgress-With Finance & IMS Team'
            )
        ),
        //Added inprogress with Receiving team -Monika
         varLoggedInUserGroup = "ReceivingTeam",
        CountRows(
            Filter(
                col_STGP_Part_Details,
                IWH_STGP_RequestNo.STGP_Status = IWH_STGP_Status.'Return Pending'||IWH_STGP_RequestNo.STGP_Status =IWH_STGP_Status.'GP Partial Return'||IWH_STGP_RequestNo.STGP_Status =IWH_STGP_Status.'InProgress-With Receiving Team'    
            )
        ),
        //CountRows(col_STGP_Part_Details)
        CountIf(col_STGP_Part_Details,true)
    )
);//Uncommented End for pagination


ClearCollect(
    col_STGP_Part_Details,ShowColumns(
    IWH_STGP_Request_Details,GP_ShipValue,GP_NetPrice,GP_PartPrice,Part_Number,GPQuantity,IWH_STGP_RequestNo,STGP_Part_Owner,Part_Description,IWH_Stand_Alone_MM_Details,Bonded_or_Not,STGPPart_SerialNo,GP_Currency,STGP_ExchangeRate,STGP_Request_Details,STGP_PartType,STGP_Return_Quantity,STGPPart_Weight,STGP_Part_Volume,CIF_Value,'GP_BondedNo.',GP_BondedDate,STGP_Comment,GP_PartWeightType,Foreign_Currency_Value,STGP_TypeofImport,STGP_HAWBNo,STGP_UOM,'Created On',GP_STPI_Import_Appr_No,GP_STPI_Import_Appr_Dt,GP_BOE_No,GP_BOEDate,GP_DutyForgone,STGP_PartRevision)
);

